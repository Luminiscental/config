{
  "Equation": {
    "prefix": ["eqn"],
    "body": [
      "\\begin{equation*}",
      "\t$0",
      "\\end{equation*}"
    ],
    "description": "Equation environment template."
  },
  "Theorem": {
    "prefix": ["thm"],
    "body": [
      "\\begin{${1:theorem}}",
      "\t$0",
      "\\end{${1:theorem}}"
    ],
    "description": "Theorem environment template."
  },
  "Remark": {
    "prefix": ["rmk"],
    "body": [
      "\\begin{${1:remark}}",
      "\t$0",
      "\\end{${1:remark}}"
    ],
    "description": "Remark environment template."
  },
  "Align": {
    "prefix": ["algn"],
    "body": [
      "\\begin{align*}",
      "\t$0",
      "\\end{align*}"
    ],
    "description": "Align environment template."
  },
  "Enumerate": {
    "prefix": ["enmt"],
    "body": [
      "\\begin{enumerate}$1",
      "\t\\item $0",
      "\\end{enumerate}"
    ],
    "description": "Enumeration template."
  },
  "Itemize": {
      "prefix": ["itmz"],
      "body": [
          "\\begin{itemize}",
          "\t\\item $0",
          "\\end{itemize}"
      ],
      "description": "Itemize template."
  },
  "Begin": {
      "prefix": ["begn"],
      "body": [
          "\\begin{$1}",
          "\t$0",
          "\\end{$1}"
      ],
      "description": "Begin/end environment."
  }
}
